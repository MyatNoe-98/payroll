<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   
   <class name="com.jcg.spring.hibernate.pojo.Leaves" table="LEAVES">
      <id name="idleave" type="int">
         <column name="idleave" />
         <generator class="native" />
      </id>
      <property name="emp_id" type="int">
         <column name="emp_id" />
      </property>
      <property name="name" type="java.lang.String">
         <column name="name" />
      </property>
      <property name="startdate" type="java.lang.String">
         <column name="startdate" />
      </property>
      <property name="enddate" type="java.lang.String">
         <column name="enddate" />
      </property>
      <property name="day" type="java.lang.Float">
         <column name="day" />
      </property>
      <property name="type" type="java.lang.String">
         <column name="type" />
      </property>
      <property name="reason" type="java.lang.String">
         <column name="reason" />
      </property>
      
   </class>

</hibernate-mapping>
