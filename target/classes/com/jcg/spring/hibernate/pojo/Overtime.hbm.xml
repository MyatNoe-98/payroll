<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   
   <class name="com.jcg.spring.hibernate.pojo.Overtime" table="OVERTIME">
      <id name="id_ot" type="int">
         <column name="id_ot" />
         <generator class="native" />
      </id>
      <property name="emp_id" type="int">
         <column name="emp_id" />
      </property>
      <property name="name" type="java.lang.String">
         <column name="name" />
      </property>
      <property name="date" type="java.lang.String">
         <column name="date" />
      </property>
      <property name="starttime" type="java.lang.String">
         <column name="starttime" />
      </property>
      <property name="endtime" type="java.lang.String">
         <column name="endtime" />
      </property>
      <property name="hour" type="java.lang.Float">
         <column name="hour" />
      </property>
   </class>

</hibernate-mapping>
 
